<html>

	<form class="reg_form" action="../../php/register.php" method="POST">
		First Name:<br>
		<input type="text" name="name">
		<br>
		Last Name:<br>
		<input type="text" name="l_name">
		<br>
		Data de Nascimento:<br>
		<input type="text" name="birth">
		<br>
		<input type="radio" name="underage" value="Yes" id="u_age">Yes
		<br>
		<input type="radio" name="underage" value="No">No
		<br>
		Telphone number:<br>
		<input type="text" name="a_code" size="3">
		<input type="text" name="phone" size="10">
		<br>
		Rank:<br>
		<select name="rank">
			<option value="Shoshinsha"> Shoshinsha </option>
			<option value="1Kyu"> 1Kyu </option>
			<option value="1dan"> 1 dan </option>
			<option value="2Dan"> 2 dan </option>
			<option value="3dan"> 3 dan </option>
			<option value="4Dan"> 4 dan </option>
			<option value="5dan"> 5 dan </option>
			<option value="6Dan"> 6 dan </option>
			<option value="7dan"> 7 dan </option>
			<option value="8Dan"> 8 dan </option>
		</select>	
		<br>
		Class:<br>
		<select name="clas">
			<option value="Student"> Student </option>
			<option value="Sensei"> Sensei </option>
		</select>
		<br>
		Photo (Only .jpg images supported):<br>
		<input type="file" name="photo" id="photo">
		<br>
		<input type="submit" value="Register" name="submit">
	</form>

</html>
